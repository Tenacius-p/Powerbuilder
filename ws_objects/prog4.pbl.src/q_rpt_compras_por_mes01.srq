PBSELECT( VERSION(400) TABLE(NAME="compras" )  TABLE(NAME="compras_det" )  TABLE(NAME="productos" ) COLUMN(NAME="productos.nombre") COMPUTE(NAME="month(compras.fecha) as mes_compra") COMPUTE(NAME="round(compras_det.cantidad * compras_det.precio_compra, 0) as precio_total")    JOIN (LEFT="compras_det.compra"    OP ="="RIGHT="compras.compra" )    JOIN (LEFT="productos.producto"    OP ="="RIGHT="compras_det.producto" )WHERE(    EXP1 ="~"compras~".~"fecha~""   OP ="between"    EXP2 =":ini and :fin" ) ) ARG(NAME = "ini" TYPE = date)  ARG(NAME = "fin" TYPE = date) 